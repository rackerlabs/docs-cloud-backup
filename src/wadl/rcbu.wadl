<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://wadl.dev.java.net/2009/02"
             xmlns:wadl="http://wadl.dev.java.net/2009/02"
             xmlns:xlink="http://www.w3.org/1999/xlink"
             xmlns:db="http://docbook.org/ns/docbook"
             xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0"
             xmlns:xi="http://www.w3.org/2001/XInclude"
             xmlns:raxapi="http://docs.rackspace.com/volume/api/v1" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:backup="http://docs.openstack.org/common/api/v1.0">
<!--******************************************************************-->
<!--       All Resources                                              -->
<!--******************************************************************-->      
   <resources base="http://localhost">
      <resource id="version" path="v1">
         <resource id="tenant_id" path="{tenant_id}">
            <param name="tenant_id" style="template"
               type="xsd:string">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">
                  <para>The unique identifier of the tenant or
                     account.</para></wadl:doc>
            </param>
            <resource id="agent" path="agent">            
               <resource id="agent_machineAgentId" path="{machineAgentId}">
                  <param name="machineAgentId" style="template" type="xsd:integer">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">
                     <para>The unique identifier of the Cloud Backup agent.</para></wadl:doc>
                  </param>
                  <method href="#getAgentDetails"/>
                  <method href="#updateAgentBackupBehavior"/>
               </resource>
               <resource id="agent_enable" path="enable"> 
                  <method href="#enableAgent"/>
               </resource>
               <resource id="agent_encrypt" path="encrypt"> 
                  <method href="#encryptVolume"/>
               </resource>
               <resource id="agent_changeencryption" path="changeencryption"> 
                  <method href="#changeEncryptPassword"/>
               </resource>        
               <resource id="agent_delete" path="delete">
                  <method href="#deleteAgent"/>
               </resource>
               <resource id="agent_migratevault" path="migratevault">
                  <method href="#migrateVault"/>
               </resource>
               <resource path="server/{hostServerId}" id="agent_server">
                  <param name="hostServerId" style="template" type="xsd:integer">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The unique identifier of the host server where the Cloud Backup agent is running.</para></wadl:doc>
                  </param>
                  <method href="#getAgentServer"/>
               </resource>
            </resource>
            <resource id="user" path="user">
               <resource id="agents" path="agents">
                  <method href="#getUserAgents"/>
               </resource>
               <resource id="wakeupagents" path="wakeupagents">
                  <method href="#postWakeUpAgents"/>
               </resource>
            </resource>
            <resource id="backup-configuration" path="backup-configuration">
               <method href="#createBackupConfiguration"/>
               <method href="#getAllBackupConfigforUser"/>
               <resource id="backup-configuration-Id" path="{backupConfigurationId}">
                  <param name="backupConfigurationId" style="template" type="xsd:integer">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The unique identifier of the backup configuration.</para></wadl:doc>
                  </param>
                  <method href="#updateBackupConfig"/>
                  <method href="#getBackupConfig"/>
                  <method href="#deleteBackupConfig"/>   
               </resource>
               <resource id="system" path="system">
                  <resource id="machineAgentId" path="{machineAgentId}">
                     <param name="machingAgentId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>The unique identifier of the Cloud Backup agent.</para></wadl:doc>
                     </param>
                     <method href="#getAllBackupConfigforAgent"/>
                  </resource>
               </resource>
               <resource id="enable" path="enable">
                  <resource id="backupConfigurationId" path="{backupConfigurationId}">
                     <param name="backupConfigurationId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>The unique identifier of the backup configuration.</para></wadl:doc>
                     </param>
                     <method href="#enableBackupConfig"/>
                  </resource>
               </resource>
            </resource>
            <resource id="backup" path="backup">
               <resource id="backup-action-requested" path="action-requested" > 
                  <method href="#startStopBackupManually"/>
               </resource>   
               <resource id="backup__backupId" path="{backupId}" >
                  <param name="backupId" style="template" type="xsd:integer">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The unique identifier of a backup.</para>
                     </wadl:doc>  
                  </param>
                  <method href="#getBackupDetails"/>
               </resource>            
               <resource id="backup_completed_backupConfigurationId" path="completed">
                  <resource id="completedBackupConfigurationId" path="{backupConfigurationId}">
                     <param name="backupConfigurationId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The unique identifier of the backup configuration.</para>
                        </wadl:doc>
                     </param>
                     <method href="#getCompletedBackups"/>
                  </resource>                   
                </resource>
               <resource id="backup_report" path="report" >
                  <resource id="backup_report_backupId" path="{backupId}" >
                     <param name="backupId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>The unique identifier of a backup.</para>
                        </wadl:doc>  
                     </param>
                     <method href="#getBackupReport"/>
                  </resource>     
               </resource>                      
            </resource>     
            <resource id="restore" path="restore">
               <method href="#createRestoreConfig"/>
               <method href="#updateRestoreConfig"/>
               <resource id="files" path="files">
                  <method href="#createRestoreFile"/>
                  <resource id="restoreId" path="{restoreId}" >
                     <param name="restoreId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>The unique identifier for a restore.</para>
                        </wadl:doc>  
                     </param>
                     <method href="#getRestoreFiles"/>
                     <method href="#deletetRestoreConfig"/>
                  </resource>       
               </resource>
            </resource>
            <resource id="backupAvailableForRestore" path="backup/availableforrestore">
               <method href="#getAvailableBackups"/>
            </resource>
            <resource id="restoreOperations" path="restore">
               <resource id="restore_action-requested" path="action-requested" >
                  <method href="#postRestoreAction"/>
               </resource>
               <resource id="restore_restoreId" path="{restoreId}" >
                  <param name="restoreId" style="template" type="xsd:integer">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The unique identifier for a restore.</para>
                     </wadl:doc>  
                  </param>
                  <method href="#getRestoreDetails"/>                  
               </resource>   
               <resource id="restore_report" path="report" >
                  <resource id="restore_report_restoreId" path="{restoreId}" >
                     <param name="restoreId" style="template" type="xsd:integer">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>The unique identifier for a restore.</para>
                        </wadl:doc>  
                     </param>
                     <method href="#getRestoreReport"/>
                  </resource>   
               </resource> 
            </resource>
            <resource path="system/activity/{agentId}" id="system_activity_agentId">
               <param name="agentId" style="template" type="xsd:integer">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The unique identifier for an agent.</para>
                  </wadl:doc>  
               </param>
               <method href="#getAgentActivity"/>
            </resource>
            <resource path="activity" id="activity">
               <method href="#getUserActivity"/> 
            </resource>  
         </resource>
      </resource>
   </resources>  
<!--******************************************************************-->
<!--       All methods                                                -->
<!--******************************************************************-->            
<!--******************************************************************-->
<!--       Agent methods                                              -->
<!--******************************************************************--> 
<!--**********************-->
<!-- Get agent details    -->
<!--**********************-->                    
         <method name="GET" id="getAgentDetails">
            <wadl:doc title="List agent details">
               <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Lists details about the machine and its agent.</para>
               <note xmlns="http://docbook.org/ns/docbook"><para>If the agent <code>Status</code> is showing as <code>Offline</code>, it is possible that the Cloud Backup agent might be idle and not posting any heartbeats. You can wake up your Cloud Backup agent with the wake up agents operation. You should wait 10-20 seconds after using this operation
                  to determine the online status of the agent reliably.</para></note>
            </wadl:doc>            
            <request>
            <representation mediaType="application/http" element="backup:Backup">
               <doc title="List agent details request">
                  <xsdxt:code href="../wadl/samples/getAgentDetails-json-http_request.txt"/>
               </doc>
            </representation>
            </request>
            <response status="200">
               <doc title="OK" xml:lang="EN">The request succeeded.</doc>
               <representation mediaType="application/json" element="backup:Backup">
                  <doc>
                     <xsdxt:code href="../wadl/samples/getAgentDetails_response.json"/>
                  </doc>
               </representation>
            </response>            
            <response status="400">
               <doc title="Bad request" xml:lang="EN">There were one or more errors in the request.</doc>
               <representation mediaType="application/json"/>              
            </response>
            <response status="401">
               <doc title="Unauthorized" xml:lang="EN">The supplied token was not authorized to access the resources. Either it is expired or invalid.</doc>
               <representation mediaType="application/json"/>
            </response>
            <response status="403">
               <doc title="Forbidden" xml:lang="EN">Access to the requested resource was denied.</doc>
               <representation mediaType="application/json"/>
               </response>
            <response status="404">
               <doc title="Not Found" xml:lang="EN">The backend services did not find anything matching the request URI.</doc>
               <representation mediaType="application/json">                  
               </representation>
            </response>
            <response status="500">
               <doc title="Instance Fault" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
               <representation mediaType="application/json"/>              
            </response>
            <response status="503">
               <doc title="Service Unavailable" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
               <representation mediaType="application/json"/>               
            </response>
         </method>
<!--*************************-->
<!-- Enable or disable agent -->
<!--*************************-->        
            <method name="POST" id="enableAgent">
               <wadl:doc title="Enable or disable an agent">
                  <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">
                     Enables or disables an agent.
                     Disabling an agent does not delete it or its data.
                     You can re-enable disabled agents later.
                  </para>
                  <note xmlns="http://docbook.org/ns/docbook"><para>Encryption is important for keeping your data confidential. 
                     However, encryption is also expensive. It takes significantly longer to backup and restore encrypted data. 
                     Consider if the data you are storing must be encrypted. If not, it is best to proceed without encryption. 
                     Encryption applies across the board, and once you encrypt a backup server, there is no turning back.</para></note>
               </wadl:doc>
               <request>
                  <representation mediaType="application/json" element="backup:Backup">
                     <doc title="Enable an agent: JSON request">
                        <xsdxt:code href="../wadl/samples/enableAgent_request.json"/>
                     </doc>
                  </representation>
                  <representation mediaType="application/json" element="backup:Backup">
                     <doc title="Disable an agent: JSON request">
                        <xsdxt:code href="../wadl/samples/disableAgent_request.json"/>
                     </doc>
                  </representation>
               </request>
               <response status="204">
                  <doc title="No Content" xml:lang="EN">The request succeeded.</doc>
                  <representation mediaType="application/json"/>
               </response>            
               <response status="400">
                  <doc title="Bad request" xml:lang="EN">There were one or more errors in the request.</doc>
                  <representation mediaType="application/json"/>              
               </response>
               <response status="401">
                  <doc title="Unauthorized" xml:lang="EN">The supplied token was not authorized to access the resources. Either it is expired or invalid.</doc>
                  <representation mediaType="application/json"/>
               </response>
               <response status="403">
                  <doc title="Forbidden" xml:lang="EN">Access to the requested resource was denied.</doc>
                  <representation mediaType="application/json"/>
               </response>
               <response status="500">
                  <doc title="Instance Fault" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
               <response status="503">
                  <doc title="Service Unavailable" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
            </method>           
<!--**************************-->
<!-- Enable volume encryption -->
<!--**************************-->        
            <method name="POST" id="encryptVolume">
               <wadl:doc title="Enable volume encryption">
                  <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Enables volume encryption.</para>
                  <para xmlns="http://docbook.org/ns/docbook">This operation enables volume encryption with AES-256 encryption if it is not already enabled. 
                     If you need assistance generating your encrypted key, see "Generating Your Encrypted Key In Cloud Backup" in the Rackspace Knowledge Center.
                  </para>
                  <note xmlns="http://docbook.org/ns/docbook"><para>After encryption is turned on, you cannot turn it off. This is a security measure. If anyone 
                     ever gained access to your account, they would not be able to access your backups without your passphrase.</para></note>
                  <para xmlns="http://docbook.org/ns/docbook">If you lose or forget your passphrase, you will not be able to recover your encrypted backups.</para>
               </wadl:doc>
               <request>
                  <representation mediaType="application/json" element="backup:Backup">
                     <doc>
                        <xsdxt:code href="../wadl/samples/encryptVolume_request.json"/>
                     </doc>
                  </representation>                  
               </request>
               <response status="200">
                  <doc title="OK" xml:lang="EN">The request succeeded.</doc>                 
                  <representation mediaType="application/text" element="backup:Backup">
                     <doc title="Enable volume encryption response">
                        <xsdxt:code href="../wadl/samples/encryptVolume_response.txt"/>
                     </doc>
                  </representation>
               </response>            
               <response status="400">
                  <doc title="Bad request" xml:lang="EN">There were one or more errors in the request.</doc>
                  <representation mediaType="application/json"/>              
               </response>
               <response status="401">
                  <doc title="Unauthorized" xml:lang="EN">The supplied token was not authorized to access the resources. Either it is expired or invalid.</doc>
                  <representation mediaType="application/json"/>
               </response>
               <response status="403">
                  <doc title="Forbidden" xml:lang="EN">Access to the requested resource was denied.</doc>
                  <representation mediaType="application/json"/>
               </response>
               <response status="500">
                  <doc title="Instance Fault" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
               <response status="503">
                  <doc title="Service Unavailable" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
            </method>

            <resource path="agent/changeencryption"
               id="changeencryption">
               <method name="POST">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN" title="Change encryption password">
                     <para role="shortdesc">Changes the encryption
                        password.</para>
                     <note xmlns="http://docbook.org/ns/docbook">
                        <para>Once encryption is turned on, it cannot
                           be turned off. This is a security measure.
                           If anyone ever gained access to your
                           account, they would not be able to access
                           your backups without your pass phrase. If
                           you lose or forget your pass phrase, you
                           will not be able to recover your encrypted
                           backups.</para>
                     </note>
                  </wadl:doc>
                  <request>
                     <representation mediaType="application/json">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">
                           <xsdxt:code
                              href="samples/encryption-change-request.json"
                           />
                        </wadl:doc>
                     </representation>
                  </request>
                  <response>
                     <representation mediaType="application/json">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">
                           <xsdxt:code
                              href="samples/encryption-change-response.txt"
                           />
                        </wadl:doc>
                     </representation>
                  </response>
                  <response status="400">
                     <representation mediaType="application/json">
                        <doc xml:lang="EN">Bad request</doc>
                     </representation>
                  </response>
                  <response status="401">
                     <representation mediaType="application/json">
                        <doc xml:lang="EN">Unauthorized</doc>
                     </representation>
                  </response>
                  <response status="403">
                     <representation mediaType="application/json">
                        <doc xml:lang="EN">Forbidden</doc>
                     </representation>
                  </response>
                  <response status="503">
                     <representation mediaType="application/json">
                        <doc xml:lang="EN">Service Unavailable</doc>
                     </representation>
                  </response>

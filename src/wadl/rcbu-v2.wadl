   <!--********************************-->
   <!--       Update a cleanup         -->
   <!--********************************-->
   <method name="PATCH" id="updateCleanup">

      <wadl:doc xml:lang="EN" title="Update a cleanup"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Updates the specified cleanup.</para>
         <para>This operation updates the specified cleanup. Cleanups are updated with the JSON Patch. For more information about the JSON Patch, see <link
            xlink:href="http://tools.ietf.org/html/rfc6902">RFC6902</link>.</para>
            <para>You can issue updates only for the following scenarios:</para>
            <itemizedlist>
               <listitem><para>The agent is reporting the state of the cleanup. For example, <code>[{ "op": "replace", "path": "/state", "value": "queued" }]</code>. The following values are valid for <code>value</code>:</para>
               <itemizedlist>
                  <listitem><para><code>queued</code></para></listitem>
                  <listitem><para><code>in_progress</code></para></listitem>
               </itemizedlist>
               </listitem>
               <listitem><para>The agent is reporting the results of the finished cleanup (see the example request in this section). The following values are valid for <code>/state</code>:</para>
                  <itemizedlist>
                  <listitem><para><code>completed</code></para></listitem>
                  <listitem><para><code>completed_with_errors</code></para></listitem>
                  <listitem><para><code>failed</code></para></listitem>
                  <listitem><para><code>stopped</code></para></listitem>
               </itemizedlist>
               </listitem>
               <listitem><para>You are issuing a request to stop the cleanup. For example, <code> [{ "op": "replace", "path": "/state", "value": "stop_requested" }</code>].</para></listitem>
            </itemizedlist>
            <para>You can use the <code>add</code> and <code>replace</code> operations interchangeably because they are interpreted identically for these scenarios.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateCleanup_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateCleanup_request.json"/>
            </doc>
            <param name="op" style="plain" path="$.op" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">The <code>replace</code> or <code>add</code> operation to update the cleanup.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path.
               </wadl:doc>
            </param>
            <param name="value" style="plain" path="$.value" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Value related to the <code>path</code>. See the beginning of this section for additional information.
               </wadl:doc>
            </param>
            <param name="count" style="plain" path="$.value.count" required="false" >
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Number of errors, when <code>path</code> is <code>/errors</code>.
                      </wadl:doc>
                   </param>
                  <param name="reason" style="plain" path="$.value.reason" required="false">
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Cause of the error, when <code>path</code> is <code>/errors</code>; for example, <code>error_deleting_object</code>.
                      </wadl:doc>
                   </param>
                  <param name="diagnostics" style="plain" path="$.value.diagnostics" required="false">
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Additional information about the cause of the error, when <code>path</code> is <code>/errors</code>.
                      </wadl:doc>
                   </param>
            <param name="list" style="plain" path="$.value.list" required="false">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Additional information about the error, when <code>path</code> is <code>/errors</code>.
                     </wadl:doc>
                  </param>
            <param name="index" style="plain" path="$.value.list.index" required="false" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Index.
                              </wadl:doc>
                           </param>
            <param name="path" style="plain" path="$.value.list.path"  required="false">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Path to the log.
                              </wadl:doc>
                           </param>
            <param name="type" style="plain" path="$.value.list.type" required="false">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Type of error.
                              </wadl:doc>
                           </param>
            <param name="exception" style="plain" path="$.value.list.exception" required="false">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Information about the exception.
                              </wadl:doc>
                           </param>
         </representation>
      </request>

      <response status="204">
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc title="Update a cleanup: 204 response">
               <xsdxt:code href="../wadl/samples-v2/cbu-updateCleanup_response-json-http.txt"/>
            </doc>
            <doc title="Update a cleanup: 409 response">
               <xsdxt:code href="../wadl/samples-v2/cbu-updateCleanup_response409-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateCleanup_response409.json"/>
            </doc>
         </representation>
      </response>
      &commonFaults;
   </method>



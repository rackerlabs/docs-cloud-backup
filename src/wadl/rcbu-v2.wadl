         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getProjectBackups_response-json-http.txt"/>
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getProjectBackups_response.json"/>
            </wadl:doc>
            <param name="backups" style="plain" path="$.backups">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the backups for the project.
               </wadl:doc>
            </param>
                  <param name="project_id" style="plain" path="$.backups.project_id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID of the project.
                     </wadl:doc>
                  </param>
                  <param name="id" style="plain" path="$.backups.id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID of the backup.
                     </wadl:doc>
                  </param>
                  <param name="agent" style="plain" path="$.backups.agent">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Information about the agent for the backup.
                     </wadl:doc>
                  </param>
                        <param name="id" style="plain" path="$.backups.agent.id">
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN">ID of the agent.
                           </wadl:doc>
                        </param>
                        <param name="links" style="plain" path="$.backups.agent.links">
                           <wadl:doc xmlns="http://docbook.org/ns/docbook"

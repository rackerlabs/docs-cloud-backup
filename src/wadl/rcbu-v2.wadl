   <!--**************************************-->
   <!--       Update a configuration         -->
   <!--**************************************-->
   <method name="PATCH" id="updateConfig">

      <wadl:doc xml:lang="EN" title="Update a configuration"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Updates the specified configuration.</para>
         <para>This operation updates the specified configuration. Configurations are updated with the JSON Patch. For more information about the JSON Patch, see <link
            xlink:href="http://tools.ietf.org/html/rfc6902">RFC6902</link>.</para>

            <para>You can use only the <code>replace</code> operation to update a configuration.</para>
            <para>You can issue updates only for the following paths:</para>
            <itemizedlist>
               <listitem><para><code>/name</code></para> </listitem>
               <listitem><para><code>/enabled</code></para></listitem>
               <listitem><para><code>/schedule</code></para></listitem>
               <listitem><para><code>/retention/days</code></para></listitem>
               <listitem><para><code>/inclusions</code></para></listitem>
               <listitem><para><code>/exclusions</code></para></listitem>
               <listitem><para><code>/notifications</code></para></listitem>
            </itemizedlist>
           <para>The restrictions that apply to the create configuration operation (see "Create a configuration) also apply to this operation as well.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateConfiguration_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateConfiguration_request.json"/>
            </doc>
            <param name="op" style="plain" path="$.op" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">The <code>replace</code> operation to update the configuration.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path to item to change in the configuration. See the beginning of this section for the valid values.
               </wadl:doc>
            </param>
            <param name="value" style="plain" path="$.value" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Value to which you want to change the information in the <code>path</code>.
               </wadl:doc>
            </param>

         </representation>
      </request>

      <response status="204">
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateConfiguration_response-json-http.txt"/>
            </doc>
         </representation>
      </response>
      &commonFaults;
   </method>


   <!--***************************************-->
   <!--       Create a configuration          -->
   <!--***************************************-->
   <method name="POST" id="postConfiguration">
      <wadl:doc xmlns="http://docbook.org/ns/docbook" title="Create a configuration">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Creates a configuration. </para>
         <para xmlns="http://docbook.org/ns/docbook">This operation creates a configuration. </para>

         <para>The following restrictions apply to the <code>schedule</code> parameter:</para>
         <itemizedlist>
            <listitem><para>Only a single <code>recurrence</code> within a <code>schedule</code> is currently supported.</para></listitem>
            <listitem><para>You must specify <code>time_zone</code> according to the <link
               xlink:href="http://www.iana.org/time-zones">Internet Assigned Numbers Authority (IANA)</link> .</para></listitem>
            <listitem><para>If <code>null</code> is provided for <code>schedule</code>, backups must be manually started for the
               configuration.</para></listitem>
         </itemizedlist>
         <note xmlns="http://docbook.org/ns/docbook"><para>Backups will be retained forever if <code>0</code> is provided to <code>retention/days</code>..</para></note>
         <para>Following are some example schedules:</para>
         <itemizedlist>
            <listitem><para>Hourly: <code>RRULE:FREQ=HOURLY;INTERVAL=2</code></para></listitem>
            <listitem><para>Daily: <code>RRULE:FREQ=DAILY;INTERVAL=1;BYHOUR=14;BYMINUTE=0</code></para></listitem>
            <listitem><para>Weekly: <code>RRULE:FREQ=WEEKLY;INTERVAL=1;BYDAY=TH;BYHOUR=14;BYMINUTE=0</code></para></listitem>
         </itemizedlist>
         <para>The following restrictions apply to the <code>inclusions</code> parameter:</para>
            <itemizedlist>
               <listitem><para>Multiple entries with the same <code>path</code> are forbidden.</para></listitem>
               <listitem><para>Entries with the same <code>path</code> as an entry in <code>exclusions</code> are forbidden.</para></listitem>
               <listitem><para>Files or folders cannot be included under existing inclusion rules.</para></listitem>
               <listitem><para>Files or folders cannot be included under existing exclusion rules.</para></listitem>
            </itemizedlist>
            <para>The following restrictions apply to the <code>exclusions</code> parameter:</para>
            <itemizedlist>
               <listitem><para>Multiple entries with the same <code>path</code> are forbidden.</para></listitem>
               <listitem><para>Entries with the same <code>path</code> as an entry in <code>inclusions</code> are forbidden.</para></listitem>
               <listitem><para>Files or folders cannot be excluded under existing exclusion rules.</para></listitem>
               <listitem><para>Each exclusion <code>path</code> must be included under an inclusion path.</para></listitem>
            </itemizedlist>
         <note xmlns="http://docbook.org/ns/docbook"><para>At least one notifications must specify <code>on_failure</code> as <code>true</code>.</para></note>
      </wadl:doc>



      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postConfig_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postConfig_request.json"/>
            </doc>
            <param name="agent_id" style="plain"
               required="true" path="$.agent_id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the agent.
               </wadl:doc>
            </param>
            <param name="name" style="plain" required="true" path="$.name">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Name of the configuration.
               </wadl:doc>
            </param>
            <param name="enabled" style="plain" required="true" path="$.enabled">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
               xml:lang="EN">Specifies whether the configuration is enabled.
            </wadl:doc>
            </param>
            <param name="schedule" style="plain" required="true" path="$.schedule">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the schedule associated with the configuration. If you specify <code>null</code> for <code>schedule</code>, you must manually start backups for the configuration.
               </wadl:doc>
            </param>
                      <param name="recurrence" style="plain" required="true" path="$.schedule.recurrence">
                         <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">Any recurrence in the configuration's schedule. Only a single <code>recurrence</code> value within <code>schedule</code> is currently supported.
                         </wadl:doc>
                      </param>
                     <param name="time_zone" style="plain" required="true" path="$.schedule.time_zone">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Time zone for the configuration. You must specify <code>time_zone</code> according to the <link
                              xlink:href="http://www.iana.org/time-zones">IANA Time Zone Database</link>.
                        </wadl:doc>
                     </param>
            <param name="retention" style="plain" required="true" path="$.retention">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information defined for retention for the configuration.
               </wadl:doc>
            </param>
                     <param name="days" style="plain" required="true" path="$.retention.days">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Number of days to retain the backup. When you set <code>days</code> to <code>0</code>, backups are retained forever.
                        </wadl:doc>
                     </param>
            <param name="inclusions" style="plain" required="true" path="$.inclusions">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about what to include in the backup. See the beginning of this section for restrictions.

               </wadl:doc>
            </param>
                     <param name="type" style="plain" required="true" path="$.inclusions.type">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">The type of the object to include: <code>file</code> or <code>folder</code>.
                        </wadl:doc>
                     </param>
                     <param name="path" style="plain" required="true" path="$.inclusions.path">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">The path to the object to include.
                        </wadl:doc>
                     </param>
                     <param name="path_encoded" style="plain" required="true" path="$.inclusions.path_encoded">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">The encoded path to the object to include.
                        </wadl:doc>
                     </param>
            <param name="exclusions" style="plain" required="true" path="$.exclusions">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about what to exclude from the backup. See the beginning of this section for restrictions.

               </wadl:doc>
            </param>
                      <param name="type" style="plain" required="true" path="$.exclusions.type">
                         <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">The type of the object to exclude: <code>file</code> or <code>folder</code>.
                         </wadl:doc>
                      </param>
                      <param name="path" style="plain" required="true" path="$.exclusions.path">
                         <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">The path to the object to exclude.
                         </wadl:doc>
                      </param>
                      <param name="path_encoded" style="plain" required="true" path="$.exclusions.path_encoded">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">The encoded path to the object to exclude.
                         </wadl:doc>
                      </param>
            <param name="notifications" style="plain" required="true" path="$.notifications">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about notifications. Note that least one notification must specify <code>on_failure</code> as <code>true</code>.
               </wadl:doc>
            </param>
                     <param name="type" style="plain" required="true" path="$.notifications.type">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">The type of the notification.
                        </wadl:doc>
                     </param>
                     <param name="destination" style="plain" required="true" path="$.notifications.destination">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Where to send the notification.
                        </wadl:doc>
                     </param>
                    <param name="on_success" style="plain" required="true" path="$.notifications.on_success">
                       <wadl:doc xmlns="http://docbook.org/ns/docbook"
                          xml:lang="EN">Specifies whether to send the notification if the backup is successful.
                       </wadl:doc>
                    </param>
                    <param name="on_failure" style="plain" required="true" path="$.notifications.on_failure">
                       <wadl:doc xmlns="http://docbook.org/ns/docbook"
                          xml:lang="EN">Specifies whether to send the notification if the backup is not successful.
                       </wadl:doc>
                    </param>
         </representation>
      </request>

      <response status="201">
         <doc title="Created" xml:lang="EN"/>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <xsdxt:code

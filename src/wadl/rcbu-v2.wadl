   <method name="GET" id="getProjectAgents">
      <wadl:doc xmlns="http://docbook.org/ns/docbook" title="List agents for a project">
         <para role="shortdesc">Lists the agents for the specified project. </para>
         <para>This operation lists all of the agents for a project.</para>
         <para>If no agents have been created for the project, the <code>agents</code> parameter in the response is an empty array.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getProjectAgents_request-json-http.txt"/>
            </doc>

            <param name="host_machine_id" style="query" type="xsd:string" required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the host machine on which the agent resides. For example, <code>8f135b4f-7a69-4b8a-947f-5e80d772fd97</code>.
               </wadl:doc>
            </param>
         </representation>
      </request>

      <response status="200">
         <doc title="OK" xml:lang="EN"/>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getProjectAgents_response-json-http.txt"/>
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getProjectAgents_response.json"/>
            </wadl:doc>
            <param name="agents" style="plain" path="$.agents">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the agents for the project.
               </wadl:doc>
            </param>
                  <param name="project_id" style="plain" path="$.agents.project_id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Project ID.
                     </wadl:doc>
                  </param>
                  <param name="id" style="plain" path="$.agents.id">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Host machine ID.
                     </wadl:doc>

   <!--********************************-->
   <!--    List backups for project     -->
   <!--********************************-->
   <method name="GET" id="getBackups">
      <wadl:doc xmlns="http://docbook.org/ns/docbook" title="List the backups for a project">
         <para role="shortdesc">Lists the backups for a project. </para>
         <para>This operation lists the backups for the specified project.</para>
         <para>If no backups have been created for the project, the <code>backups</code> parameter in the response is an empty array.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getProjectBackups_request-json-http.txt"/>
            </doc>
            <param name="marker" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the last known backup, for example, <code>0d95d699-d16b-11e4-93bd-c8e0eb190e3d</code>.</wadl:doc>
            </param>
            <param name="limit" style="query" type="xsd:integer"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Number of backups to list. The default
                  value is 100.
               </wadl:doc>
            </param>
            <param name="sort_dir" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
               </wadl:doc>
            </param>
            <param name="restorable" style="query" type="xsd:boolean"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Indicates whether only restorable backups are returned. If <code>restorable</code> is <code>true</code>, only restorable backups are returned.
               </wadl:doc>
            </param>
            <param name="configuration_id" style="query" type="xsd:string"
               required="false">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Use only in conjunction with <code>restorable=true</code>. If you provide <code>configuration_id</code>, only restorable backups for the specified configuration are returned; for example, <code>-568f-4d5a-932f-fb2af86b5fd5</code>.
               </wadl:doc>
            </param>
         </representation>
      </request>

      <response status="200">

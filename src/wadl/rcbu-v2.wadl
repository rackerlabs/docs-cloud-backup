   <!--********************************-->
   <!--        Update a logfile        -->
   <!--********************************-->
   <method name="PATCH" id="updateLogfile">

      <wadl:doc xml:lang="EN" title="Update a log file"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Updates the specified log file for the specified agent.</para>
         <para>This operation updates the specified log file for the specified agent. Agents update the state of a log file by updating the <code>/state</code> path via a JSON Patch document. The <code>/state</code> path is the only path that can be modified. Only the <code>replace</code> operation is permitted. For more information about the JSON Patch, see <link
            xlink:href="http://tools.ietf.org/html/rfc6902">RFC6902</link>.</para>
         <para>The initial state when the log file is requested is <code>requested</code>. Agents update <code>/state</code> with the values <code>started</code> or <code>completed</code>.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateLogfile_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateLogfile_request.json"/>
            </doc>
            <param name="op" style="plain" path="$.op" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">The <code>replace</code> operation to update the log file.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path. The valid value is <code>/state</code>.
               </wadl:doc>
            </param>
            <param name="value" style="plain" path="$.value" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Value related to the <code>path</code>. Valid values are <code>started</code> and <code>completed</code>.
               </wadl:doc>
            </param>
         </representation>
      </request>

      <response status="204">
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateLogfile_response-json-http.txt"/>
            </doc>
         </representation>

      </response>
      &commonFaults;
   </method>


   <!--********************************-->
   <!--        Update a backup         -->
   <!--********************************-->
   <method name="PATCH" id="updateBackup">

      <wadl:doc xml:lang="EN" title="Update a backup"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Updates the specified backup.</para>
         <para>This operation updates the specified backup. Backups are updated with the <link
            xlink:href="http://tools.ietf.org/html/rfc6902">JSON Patch</link>.</para>

         <para>You can issue updates for the following scenarios only:</para>
            <itemizedlist>
               <listitem><para>The agent is reporting the state of the backup; for example, <code>[{ "op": "replace", "path": "/state", "value": "queued" }]</code>. The following values are valid for <code>value</code>:</para>
               <itemizedlist>
                  <listitem><para><code>queued</code></para></listitem>
                  <listitem><para><code>preparing</code></para></listitem>
                  <listitem><para><code>in_progress</code></para></listitem>
               </itemizedlist>
               </listitem>
               <listitem><para>The agent is reporting the results of the finished backup (see the example request in this section). The following values are valid for <code>/state</code>:</para>
                  <itemizedlist>
                  <listitem><para><code>completed</code></para></listitem>
                  <listitem><para><code>completed_with_errors</code></para></listitem>
                  <listitem><para><code>failed</code></para></listitem>
                  <listitem><para><code>stopped</code></para></listitem>
                  <listitem><para><code>skipped</code></para></listitem>
               </itemizedlist>
               </listitem>
               <listitem><para>You are issuing a request to stop the backup; for example, <code> [{ "op": "replace", "path": "/state", "value": "stop_requested" }</code>].</para></listitem>
            </itemizedlist>
            <para>You can use the <code>add</code> and <code>replace</code> operations interchangeably because they are interpreted identically for these scenarios.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_request.json"/>
            </doc>
            <param name="op" style="plain" path="$.op" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">The <code>replace</code> or <code>add</code> operation to update the backup.
               </wadl:doc>
            </param>
            <param name="path" style="plain" path="$.path" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Path of item to change.
               </wadl:doc>
            </param>
            <param name="value" style="plain" path="$.value" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Value related to the <code>path</code>. See the beginning of this section for additional information.
               </wadl:doc>
            </param>
                   <param name="count" style="plain" path="$.value.count" >
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Number of errors, when <code>path</code> is <code>/errors</code>.
                      </wadl:doc>
                   </param>
                   <param name="reason" style="plain" path="$.value.reason" >
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Cause of the error, when <code>path</code> is <code>/errors</code>; for example, <code>unable_to_process_some_files</code>.
                      </wadl:doc>
                   </param>
                   <param name="diagnostics" style="plain" path="$.value.diagnostics" >
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Additional information about the cause of the error, when <code>path</code> is <code>/errors</code>; for example, <code>Some files could not be backed up. Partial list follows.</code>
                      </wadl:doc>
                   </param>
                  <param name="list" style="plain" path="$.value.list" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Additional information about the cause of the error, when <code>path</code> is <code>/errors</code>.
                     </wadl:doc>
                  </param>
                           <param name="index" style="plain" path="$.value.list.index" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Index for the error.
                              </wadl:doc>
                           </param>
                           <param name="path" style="plain" path="$.value.list.path" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Path to the log.
                              </wadl:doc>
                           </param>
                           <param name="type" style="plain" path="$.value.list.type" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Type of error.
                              </wadl:doc>
                           </param>
                           <param name="exception" style="plain" path="$.value.list.exception" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Information about the exception.
                              </wadl:doc>
                           </param>
                                  <param name="code" style="plain" path="$.value.list.exception.code" >
                                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                        xml:lang="EN">Exception code.
                                     </wadl:doc>
                                  </param>
                                  <param name="description" style="plain" path="$.value.list.exception.description" >
                                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                        xml:lang="EN">Description of the exception.
                                     </wadl:doc>
                                  </param>
                                  <param name="details" style="plain" path="$.value.list.exception.details" >
                                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                        xml:lang="EN">Additional details about the exception.
                                     </wadl:doc>
                                  </param>
         </representation>
      </request>

      <response status="204">
         <doc title="No Content" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc title="Update a backup: 204 response">
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_response-json-http.txt"/>
            </doc>
            <doc title="Update a backup: 409 response">
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_response409-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_response409.json"/>
            </doc>
         </representation>
      </response>
<!--      <response status="409">
         <doc title="Conflict" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_response409-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-updateBackup_response409.json"/>
            </doc>
         </representation>
      </response>     -->
      &commonFaults;
   </method>


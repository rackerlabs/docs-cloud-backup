   <!--*******************************-->
   <!--     Authenticate an agent     -->
   <!--*******************************-->
   <method name="POST" id="postAgentTokens">
      <wadl:doc xml:lang="EN" title="Authenticate an agent"  xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Authenticates an agent.</para>
         <para>This operation authenticates an agent.</para>
         <para>Agents authenticate with the API and retrieve an authentication token. The authentication token
            returned is the token issued by Cloud Identity for the user who originally registered the agent.
            The token is used for communicating with the Cloud Backup API and other products associated with the
            same Cloud Identity server.</para>
         <para>The following error conditions are returned if the request is not successful:<itemizedlist>
            <listitem><para>401 (Unauthorized) is returned if the provided credentials are rejected.</para></listitem>
            <listitem><para>409 (Conflict) is returned with a message in the response body if the provided fingerprint does not match the agent's fingerprint submitted during registration. Such a case likely indicates two agents (likely on different hosts) are attempting to authenticate as one.</para></listitem>
            <listitem><para>503 (Service Unavailable) is returned with the <code>Retry-After</code> header if communication with any upstream service fails during authentication. Use the <code>Retry-After</code> header to determine when to retry authentication.</para></listitem>
         </itemizedlist>
         </para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentTokens_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentTokens_request.json"/>
            </doc>
            <param name="project_id" style="plain" path="$.project_id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the project associated with the token.
               </wadl:doc>
            </param>
            <param name="id" style="plain" path="$.id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the agent associated with the token.
               </wadl:doc>
            </param>
            <param name="password" style="plain" path="$.password" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Password associated with the token.
               </wadl:doc>
            </param>
            <param name="name" style="plain" path="$.name">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Name of the server.
               </wadl:doc>
            </param>
            <param name="version" style="plain" path="$.version">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Version of the server.
               </wadl:doc>
            </param>
            <param name="host" style="plain" path="$.host">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the host.
               </wadl:doc>
            </param>
                     <param name="os" style="plain" path="$.host.os">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Information about the operating system.
                        </wadl:doc>
                     </param>
                           <param name="name" style="plain" path="$.host.os.name">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Name of the operating system.
                              </wadl:doc>
                           </param>
                           <param name="version" style="plain" path="$.host.os.version">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Version of the operating system.
                              </wadl:doc>
                           </param>
                           <param name="architecture" style="plain" path="$.host.os.architecture" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Architecture of the operating system.
                              </wadl:doc>
                           </param>
                     <param name="fingerprint" style="plain" path="$.host.fingerprint">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Fingerprint for the agent.
                        </wadl:doc>
                     </param>
                      <param name="addresses" style="plain" path="$.host.addresses" >
                         <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">Information about the IP addresses for the agent.
                         </wadl:doc>
                      </param>
                              <param name="version" style="plain" path="$.host.addresses.version" >
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">Version for the address  (4 for IPv4 or 6 for IPv6).
                                 </wadl:doc>
                              </param>
                              <param name="addr" style="plain" path="$.host.addresses.addr">
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">IP address.
                                 </wadl:doc>
                      </param>
         </representation>
      </request>

      <response status="200">
         <doc title="OK" xml:lang="EN"/>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentTokens_response-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgentTokens_response.json"/>
            </doc>
            <param name="token" style="plain" path="$.token">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the authentication token.
               </wadl:doc>
            </param>
                     <param name="id" style="plain" path="$.token.id">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">ID of the authentication token.
                        </wadl:doc>
                     </param>
                     <param name="expires" style="plain" path="$.token.expires">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Expiration date and time for the authentication token.
                        </wadl:doc>
                     </param>
         </representation>
      </response>
      &commonFaults;
   </method>

   <!--******************************************************************-->
   <!--       Backups methods                                            -->
   <!--******************************************************************-->
   <!--********************************-->
   <!--        Start a backup          -->
   <!--********************************-->
   <method name="POST" id="postBackups">
      <wadl:doc title="Start a backup">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Starts a backup. </para>
         <para xmlns="http://docbook.org/ns/docbook">This operation starts a backup. Start backups by specifying one of the following states:
         <itemizedlist>
            <listitem><para><code>start_requested</code>: A manual backup is requested by a user.</para></listitem>
            <listitem><para><code>start_scheduled</code>: A scheduled backup is started by the agent. <code>start_scheduled</code> requires that the <code>X-Agent-Id</code> header is in the request.</para></listitem>
         </itemizedlist></para>
      </wadl:doc>

      <request>
         <param
            name="X-Agent-Id" style="header" type="xsd:string">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
               xml:lang="EN">Agent ID, which is required when you specify the state <code>start_scheduled</code>. For example, 8f135b4f-7a69-4b8a-947f-5e80d772fd97.</wadl:doc>
         </param>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postBackup_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postBackup_request.json"/>
            </doc>
            <param name="configuration_id" style="plain" path="$.configuarion_id"
               required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the configuration, for example, <code>7c8ee069-568f-4d5a-932f-fb2af86b5fd5</code>,<code>abf89721-616e-4922-94d4-29757d6a8671</code>.
               </wadl:doc>
            </param>
            <param name="state" style="plain" path="$.state" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Backup state. Either <code>start_requested</code>, a manual backup requested by a user, or <code>start_scheduled</code>, a scheduled backup started by the agent. Note that <code>start_scheduled</code> also requires that the <code>X-Agent-Id</code> header is in the request
               </wadl:doc>
            </param>
         </representation>
      </request>

      <response status="201">
         <doc title="Created" xml:lang="EN"/>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-postBackup_response-json-http.txt"/>
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-postBackup_response.json"/>
            </wadl:doc>
            <param name="project_id" style="plain" path="$.project_id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the project.
               </wadl:doc>
            </param>
            <param name="id" style="plain" path="$.id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the backup.
               </wadl:doc>
            </param>
            <param name="agent" style="plain" path="$.agent">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the agent for the backup.
               </wadl:doc>
            </param>
            <param name="id" style="plain" path="$.agent.id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the agent.
               </wadl:doc>
            </param>
            <param name="links" style="plain" path="$.agent.links">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Link information for the agent for the backup.
               </wadl:doc>
            </param>
            <param name="href" style="plain" path="$.agent.links.href" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Location (URI) of the agent.
               </wadl:doc>
            </param>
            <param name="rel" style="plain" path="$.agent.links.rel" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">How the href link provided is related to this resource URI.
               </wadl:doc>
            </param>


            <param name="configuration" style="plain" path="$.configuration" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the configuration.
               </wadl:doc>
            </param>
            <param name="id" style="plain" path="$.configuration.id">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">ID of the configuration.
               </wadl:doc>
            </param>
            <param name="links" style="plain" path="$.configuration.links">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Link information about the configuration for the backup.
               </wadl:doc>
            </param>
            <param name="href" style="plain" path="$.configuration.links.href" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"

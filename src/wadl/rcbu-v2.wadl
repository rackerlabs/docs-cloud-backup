            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getRestoreEvents_response-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-getRestoreEvents_response.json"/>
            </doc>
            <param name="events" style="plain" path="$.events" >
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about events.
               </wadl:doc>
            </param>
                  <param name="id" style="plain" path="$.events.id" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">ID of the event.
                     </wadl:doc>
                  </param>
                  <param name="time" style="plain" path="$.events.time" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Time of the event.
                     </wadl:doc>
                  </param>
                  <param name="event" style="plain" path="$.events.event" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Type of the event.
                     </wadl:doc>
                  </param>
                  <param name="agent" style="plain" path="$.events.agent" >
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">Agent information for the event.
                     </wadl:doc>
                  </param>
                          <param name="id" style="plain" path="$.events.agent.id" >
                             <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                xml:lang="EN">ID of the agent.
                             </wadl:doc>
                          </param>
                   <param name="source_agent" style="plain" path="$.events.source_agent" >
                      <wadl:doc xmlns="http://docbook.org/ns/docbook"
                         xml:lang="EN">Source agent information for the event.
                      </wadl:doc>
                   </param>
                           <param name="vault" style="plain" path="$.events.source_agent.vault" >
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Vault information for the source agent for the event.
                              </wadl:doc>
                           </param>
                                  <param name="id" style="plain" path="$.events.source_agent.vault.id" >
                                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                        xml:lang="EN">ID of the vault for the source agent for the event.
                                     </wadl:doc>
                                  </param>
                                 <param name="encrypted" style="plain" path="$.events.source_agent.vault.encrypted" >
                                    <wadl:doc xmlns="http://docbook.org/ns/docbook"

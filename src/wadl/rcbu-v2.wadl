<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application [
    <!ENTITY % common  SYSTEM "common-v2.ent">
    %common;


]>
<application xmlns="http://wadl.dev.java.net/2009/02"
             xmlns:wadl="http://wadl.dev.java.net/2009/02"
             xmlns:xlink="http://www.w3.org/1999/xlink"
             xmlns:db="http://docbook.org/ns/docbook"
             xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0"
             xmlns:xi="http://www.w3.org/2001/XInclude"
             xmlns:raxapi="http://docs.rackspace.com/volume/api/v1"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:backup2="http://docs.openstack.org/common/api/v2"
             xmlns:imageapi="http://docs.openstack.org/image/api/v2.0"
             xmlns:atom="http://www.w3.org/2005/Atom"
             xmlns:csapi="http://docs.openstack.org/compute/api/v2"
             xmlns:common="http://docs.openstack.org/common/api/v1.0"
             xmlns:rcbu="http://docs.openstack.org/database/api/v1.0">

<!--******************************************************************-->
<!--       All Resources                                              -->
<!--******************************************************************-->
   <resources base="https://dfw.backup.api.rackspacecloud.com/">
      <resource id="version" path="v2">

         <resource id="project_id" path="{project_id}">
            <param name="project_id" style="template"
               type="xsd:string">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">
                  <para>Project ID of the user. Also referred to as the tenant ID
                     or account ID.</para></wadl:doc>
                  </param>

            <!--************************************************-->
            <!--       Health/Base Resources                    -->
            <!--************************************************-->

            <resource id="health" path="health">
               <method href="#getHealthCheck"/>
               <resource id="dependent_system" path="{dependent_system}">
                  <param name="dependent_system" style="template" type="xsd:string">
                     <wadl:doc
                        xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN"><para>Unique identifier for the dependent system.</para> </wadl:doc>
                  </param>
                  <method href="#getHealthDetails"/>
               </resource>
            </resource>

            <resource id="ping" path="ping">
               <method href="#getPing"/>
            </resource>

            <!--************************************************-->
            <!--       Activities Resources                     -->
            <!--************************************************-->

            <resource id="activities" path="activities">
               <method href="#getProjectActivities"/>
            </resource>

            <!--************************************************-->
            <!--       Agents Resources                         -->
            <!--************************************************-->

            <resource id="agents" path="agents">
               <method href="#postAgent"/>
               <method href="#getProjectAgents"/>

               <resource id="agent_id" path="{agent_id}">
                  <param name="agent_id" style="template" type="xsd:string" required="true">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN">
                        <para>Agent ID. For example, <code>8f135b4f-7a69-4b8a-947f-5e80d772fd97</code>.</para>
                     </wadl:doc>
                  </param>
                  <method href="#getAgentDetails"/>
                  <method href="#updateAgent"/>
                  <method href="#deleteAgent"/>
                  <resource id="agentActivities" path="activities">
                     <method href="#getAgentActivities"/>
                  </resource>
                  <resource id="browseRequest" path="browse-requests">
                     <method href="#postAgentBrowseRequest"/>
                     <resource id="request_id" path="{request_id}">
                        <param name="request_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Browse request ID. For example, <code>16ce47f7-88b2-4983-8b1c-d4a82306ae87</code>.</para>
                           </wadl:doc>
                        </param>
                           <method href="#getAgentBrowseResult"/>
                      </resource>
                  </resource>
                  <resource id="configuration" path="configuration">
                  <method href="#getAgentConfig"/>
                  </resource>
                  <resource id="configurations" path="configurations">
                     <method href="#getAgentConfigs"/>
                  </resource>
                  <resource id="agentEvents" path="events">
                     <method href="#postAgentEvents"/>
                     <method href="#getAgentEvents"/>
                  </resource>
                  <resource id="logfiles" path="logfiles">
                     <method href="#postAgentLogfile"/>
                     <method href="#getAgentLogfiles"/>
                     <resource id="logfile_id" path="{logfile_id}">
                        <param name="logfile_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Log file ID. For example, <code>a533a845-4279-4838-af13-276114e90234</code>.</para>
                           </wadl:doc>
                        </param>
                        <method href="#getAgentLogfileDetails"/>
                        <method href="#updateLogfile"/>
                        <method href="#deleteLogfile"/>
                     </resource>
                  </resource>
                  <resource id="status" path="status">
                     <method href="#getAgentStatus"/>
                  </resource>
                  <resource id="vault" path="vault">
                     <method href="#postAgentVault"/>
                  </resource>
                  <resource id="vault-encryption-requests" path="vault-encryption-requests">
                     <method href="#postAgentVaultEncryptionRequest"/>
                     <resource id="encryption_request_id" path="{request_id}">
                        <param name="request_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Encryption request ID. For example, <code>9072bb51-d5fd-4fc5-ad80-d62e573236b6</code>.</para>
                           </wadl:doc>
                        </param>
                        <method href="#postAgentVaultEncryptionReport"/>
                        <method href="#getAgentVaultEncryptionReport"/>
                     </resource>
                  </resource>
                  <resource id="vault-password-verification-requests" path="vault-password-verification-requests">
                     <method href="#postAgentVaultPasswordRequest"/>
                     <resource id="password_request_id" path="{request_id}">
                        <param name="request_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Password verification request ID. For example, <code>f353f472-4931-463a-9920-1dcad25f88e7</code>.</para>
                           </wadl:doc>
                        </param>
                      <method href="#postAgentVaultPasswordReport"/>
                              <method href="#getAgentVaultPasswordRequest"/>
                     </resource>

                  </resource>

               </resource>
               <resource id="tokens" path="tokens">
                  <method href="#postAgentTokens"/>
               </resource>
             </resource>

            <!--************************************************-->
            <!--       Backups Resources                        -->
            <!--************************************************-->

            <resource id="backups" path="backups">
               <method href="#postBackups"/>
               <method href="#getBackups"/>

               <resource id="backup_id" path="{backup_id}">
                  <param name="backup_id" style="template" type="xsd:string" required="true">
                     <wadl:doc
                        xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN"><para>Backup ID. For example, <code>0d95d699-d16b-11e4-93bd-c8e0eb190e3d</code>.</para> </wadl:doc>
                  </param>
                  <method href="#getBackupDetails"/>
                  <method href="#updateBackup"/>

                  <resource id="backupsBrowseRequests" path="browse-requests">
                     <method href="#postBackupBrowseRequest"/>
                     <resource id="backup_request_id" path="{request_id}">
                        <param name="request_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Browse request ID. For example, <code>ae7528c8-bcc3-4356-a237-f20fbdd79ee4</code>.</para>
                           </wadl:doc>
                        </param>
                        <method href="#getBackupBrowseResult"/>
                     </resource>
                  </resource>

                  <resource id="errors" path="errors">
                     <method href="#getBackupErrors"/>
                  </resource>

                  <resource id="backupEvents" path="events">
                     <method href="#getBackupEvents"/>
                  </resource>
               </resource>
            </resource>


               <!--************************************************-->
               <!--       Cleanups Resources                        -->
               <!--************************************************-->

               <resource id="cleanups" path="cleanups">
                  <method href="#postCleanups"/>
                  <method href="#getCleanups"/>

                  <resource id="cleanup_id" path="{cleanup_id}">
                     <param name="cleanup_id" style="template" type="xsd:string" required="true">
                        <wadl:doc
                           xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN"><para>Cleanup ID. For example, <code>2f8708b3-d16b-11e4-bc22-c8e0eb190e3d</code>.</para> </wadl:doc>
                     </param>
                     <method href="#getCleanupDetails"/>
                     <method href="#updateCleanup"/>

                     <resource id="cleanupErrors" path="errors">
                        <method href="#getCleanupErrors"/>
                     </resource>

                     <resource id="cleanupEvents" path="events">
                        <method href="#getCleanupEvents"/>
                     </resource>

                  </resource>
               </resource>

                  <!--************************************************-->
                  <!--       Configurations Resources                 -->
                  <!--************************************************-->

                  <resource id="configure" path="configurations">
                     <method href="#postConfiguration"/>
                     <method href="#getConfigurations"/>

                     <resource id="configuration_id" path="{configuration_id}">
                        <param name="configuration_id" style="template" type="xsd:string" required="true">
                           <wadl:doc
                              xmlns="http://docbook.org/ns/docbook"
                              xml:lang="EN"><para>Configuration ID. For example, <code>7c8ee069-568f-4d5a-932f-fb2af86b5fd5</code>.</para> </wadl:doc>
                        </param>
                        <method href="#getConfigDetails"/>
                        <method href="#updateConfig"/>
                        <method href="#deleteConfig"/>

                        <resource id="configActivities" path="activities">
                           <method href="#getConfigActivities"/>
                        </resource>

                        <resource id="configEvents" path="events">
                           <method href="#getConfigEvents"/>
                        </resource>

                     </resource>
                  </resource>

         <!--************************************************-->
         <!--       Events Resources                         -->
         <!--************************************************-->

            <resource id="events" path="events">
               <method href="#postEvents"/>
               <method href="#getEvents"/>
            </resource>

         <!--************************************************-->
         <!--       Restores Resources                       -->
         <!--************************************************-->

            <resource id="restores" path="restores">
               <method href="#postRestores"/>
               <method href="#getRestores"/>

               <resource id="restore_id" path="{restore_id}">
                  <param name="restore_id" style="template" type="xsd:string" required="true">
                     <wadl:doc
                        xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN"><para>Restore ID. For example, <code>e87e6f7d-d166-11e4-8689-c8e0eb190e3d</code>.</para> </wadl:doc>
                  </param>
                  <method href="#getRestoreDetails"/>
                  <method href="#updateRestore"/>

                  <resource id="restoreErrors" path="errors">
                     <method href="#getRestoreErrors"/>
                  </resource>

                  <resource id="event" path="events">
                     <method href="#getRestoreEvents"/>
                  </resource>

               </resource>
             </resource>



      <!--************************************************-->
      <!--       Settings Resources                       -->
      <!--************************************************-->

      <resource id="settings" path="settings">
         <resource id="summaries" path="summaries">
            <method href="#putSummarySettings"/>
            <method href="#getSummarySettings"/>
         </resource>
      </resource>

<!--*************************************-->
<!--       Final ending resource tag     -->
<!--*************************************-->

         </resource>
      </resource>


   </resources>



<!--******************************************************************-->
<!--       All methods                                                -->
<!--******************************************************************-->
<!--                                                                  -->
<!--******************************************************************-->
<!--       Health methods                                             -->
<!--******************************************************************-->
<!--*******************************************************-->
<!--      Get health check - ?? move to Admin WADL ??      -->
<!--*******************************************************-->
	<method name="GET" id="getHealthCheck">
		<wadl:doc xml:lang="EN" title="Perform health check"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Performs a health check for all dependent systems.</para>
			<para>This operation performs a health check for all dependent systems. </para>
		</wadl:doc>

		<request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples-v2/cbu-getHealthCheck_request-json-http.txt"/>
				</doc>
			</representation>
		</request>

		<response status="200" xmlns="http://wadl.dev.java.net/2009/02">
		   <doc title="OK" xml:lang="EN">The request succeeded.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook"
				   xml:lang="EN" title="Perform health check with response code 200: JSON response">
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthCheck1_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthCheck1_response.json"
					/>
				</wadl:doc>

				      <param name="storage" style="plain" path="$.storage">
				         <wadl:doc xmlns="http://docbook.org/ns/docbook"
				            xml:lang="EN">Information about storage.
				         </wadl:doc>
				      </param>
				      <param name="cassandra" style="plain" path="$.storage.cassandra">
				         <wadl:doc xmlns="http://docbook.org/ns/docbook"
				            xml:lang="EN">Information about the Cassandra database.
				         </wadl:doc>
				      </param>
				      <param name="online" style="plain" path="$.storage.cassandra.online">
				         <wadl:doc xmlns="http://docbook.org/ns/docbook"
				            xml:lang="EN">Indicates if the Cassandra database is online.
				         </wadl:doc>
				      </param>
				      <param name="links" style="plain" path="$.storage.cassandra.links">
				         <wadl:doc xmlns="http://docbook.org/ns/docbook"
				            xml:lang="EN">Links with information about the Cassandra database.
				         </wadl:doc>
				      </param>
				         <param name="href" style="plain" path="$.storage.cassandra.links.href" >
				            <wadl:doc xmlns="http://docbook.org/ns/docbook"
				               xml:lang="EN">Location (URI).
				            </wadl:doc>
				         </param>
				         <param name="rel" style="plain" path="$.storage.cassandra.links.rel" >
				            <wadl:doc xmlns="http://docbook.org/ns/docbook"
				               xml:lang="EN">How the href link provided is related to this resource URI.
				            </wadl:doc>
				         </param>
			   <param name="transport" style="plain" path="$.transport">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Information about the transport driver.
			      </wadl:doc>
			   </param>
			   <param name="pecan" style="plain" path="$.transport.pecan">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Information about the Pecan transport driver.
			      </wadl:doc>
			   </param>
			   <param name="online" style="plain" path="$.transport.pecan.online">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Indicates if the Pecan transport driver is online.
			      </wadl:doc>
			   </param>
			   <param name="node" style="plain" path="$.transport.pecan.node">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">The node for the Pecan transport driver.
			      </wadl:doc>
			   </param>
			   <param name="links" style="plain" path="$.transport.pecan.link">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Links for the Pecan transport driver.
			      </wadl:doc>
			   </param>
			   <param name="href" style="plain" path="$.transport.pecan.link.href">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Location (URI).
			      </wadl:doc>
			   </param>
			   <param name="rel" style="plain" path="$.transport.pecan.link.rel">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">How the href link provided is related to this resource URI.
			      </wadl:doc>
			   </param>
			</representation >
		   <representation mediaType="application/json">
		      <wadl:doc xmlns="http://docbook.org/ns/docbook"
		         xml:lang="EN" title="Perform health check with response code 503: JSON response">
		         <xsdxt:code
		            href="../wadl/samples-v2/cbu-getHealthCheck2_response-json-http.txt"
		         />
		         <xsdxt:code
		            href="../wadl/samples-v2/cbu-getHealthCheck2_response.json"
		         />
		      </wadl:doc>
		   </representation>
		</response>
	   &commonFaults;
	</method>
<!--*******************************************************-->
<!--     Get health details   - ?? move to Admin WADL ??   -->
<!--*******************************************************-->
	<method name="GET" id="getHealthDetails">
		<wadl:doc xml:lang="EN" title="Get health details"
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">Gets the health details for the specified dependent system. </para>
			<para>This operation shows whether the specified dependent system is online or offline. </para>

		</wadl:doc>

	   <request>
			<representation mediaType="application/json" >
				<doc>
					<xsdxt:code href="../wadl/samples-v2/cbu-getHealthDetails_request-json-http.txt"/>
				</doc>
			</representation>
		</request>

	   <response status="200" xmlns="http://wadl.dev.java.net/2009/02">
	      <doc title="OK" xml:lang="EN">The request succeeded.</doc>
			<representation mediaType="application/json">
			   <wadl:doc xmlns="http://docbook.org/ns/docbook"
			      xml:lang="EN" title="Get health details with response code 200: JSON response">
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthDetails1_response-json-http.txt"
					/>
					<xsdxt:code
						href="../wadl/samples-v2/cbu-getHealthDetails1_response.json"
					/>
			   </wadl:doc>

			   <param name="online" style="plain" path="$.online">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Indicates if the specified dependent system is online.
			      </wadl:doc>
			   </param>
			   <param name="links" style="plain" path="$.links">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Links for the specified dependent system.
			      </wadl:doc>
			   </param>
			   <param name="href" style="plain" path="$.links.href">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">Location (URI).
			      </wadl:doc>
			   </param>
			   <param name="rel" style="plain" path="$.links.rel">
			      <wadl:doc xmlns="http://docbook.org/ns/docbook"
			         xml:lang="EN">How the href link provided is related to this resource URI.
			      </wadl:doc>
			   </param>
			</representation>
	      <representation mediaType="application/json">
	         <wadl:doc xmlns="http://docbook.org/ns/docbook"
	            xml:lang="EN" title="Get health details with response code 503: JSON response">
	            <xsdxt:code
	               href="../wadl/samples-v2/cbu-getHealthDetails2_response-json-http.txt"
	            />
	            <xsdxt:code
	               href="../wadl/samples-v2/cbu-getHealthDetails2_response.json"
	            />
	         </wadl:doc>
	      </representation>
		</response>
	   &commonFaults;
	</method>
<!--*******************************************************-->
<!--               Get ping                                -->
<!--*******************************************************-->
   <method name="GET" id="getPing">
      <wadl:doc xml:lang="EN" title="Ping the application"
         xmlns="http://docbook.org/ns/docbook">
         <para role="shortdesc">Pings the application.</para>
         <para>This operation gives a quick indication of whether the application is running. It is used by a load balancer's health checker. </para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json" >
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-getPing_request-json-http.txt"/>
            </doc>
         </representation>
      </request>

      <response status="204" xmlns="http://wadl.dev.java.net/2009/02">
         <doc title="No Content" xml:lang="EN">The server successfully fulfilled the request. There is no additional content to send in the response body.</doc>
         <representation mediaType="application/json">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
               xml:lang="EN">
               <xsdxt:code
                  href="../wadl/samples-v2/cbu-getPing_response-json-http.txt"
               />
            </wadl:doc>
         </representation>
      </response>
      &commonFaults;
   </method>

<!--******************************************************************-->
<!--       Activity method                                            -->
<!--******************************************************************-->
<!--********************************-->
<!-- List activities for project    -->
<!--********************************-->
         <method name="GET" id="getProjectActivities">
            <wadl:doc title="List activities for a project">
               <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Lists the activities for the project. </para>
               <para xmlns="http://docbook.org/ns/docbook">This operation lists the activities for the project. If no activities are available, the <code>activities</code> parameter in the response is an empty array.</para>
            </wadl:doc>

            <request>
               <param name="marker" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">The <code>id</code> of the last known activity. For example, <code>0d95d699-d16b-11e4-93bd-c8e0eb190e3d</code>.</wadl:doc>
               </param>
               <param name="limit" style="query" type="xsd:integer"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Number of activities to list. The default
                     value is 100.
                  </wadl:doc>
               </param>
               <param name="sort_dir" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Direction to sort the results. Valid values are <code>asc</code> and <code>desc</code>. The default value is <code>desc</code>.
                  </wadl:doc>
               </param>

<!--           <remark>Removed 4/2 per email from Bryan</remark>
               <param name="type" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Type of activities to filter. Valid values are <code>backup</code>, <code>cleanup</code>, or <code>restore</code>.
                     </wadl:doc>
               </param>
               <param name="state" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">State of the activities to filter. Valid values are any state defined by a backup, a cleanup, or a restore, such as <code>completed_with_errors</code>.
                  </wadl:doc>
               </param>
               <param name="agent_ids" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Comma-separated list of agent IDs by which to filter. For example, <code>8f135b4f-7a69-4b8a-947f-5e80d772fd97</code>,<code>6d94a88d-1c17-484b-9247-b76f731f4731</code>.
                  </wadl:doc>
               </param>
               <param name="configuration_ids" style="query" type="xsd:string"
                  required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                     xml:lang="EN">Comma-separated list of configuration IDs by which to filter. For example, <code>7c8ee069-568f-4d5a-932f-fb2af86b5fd5</code>,<code>abf89721-616e-4922-94d4-29757d6a8671</code>.
                  </wadl:doc>
               </param>
-->
               <representation mediaType="application/json" >
                  <doc>
                     <xsdxt:code href="../wadl/samples-v2/cbu-getProjectActivities_request-json-http.txt"/>
                  </doc>
               </representation>
            </request>

            <response status="200">
               <doc title="OK" xml:lang="EN">The request succeeded.</doc>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                  <xsdxt:code
                     href="../wadl/samples-v2/cbu-getProjectActivities_response-json-http.txt"

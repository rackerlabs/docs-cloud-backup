   <method name="POST" id="postAgent">
      <wadl:doc title="Register an agent">
         <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Registers an agent. </para>
         <para xmlns="http://docbook.org/ns/docbook">This operation registers an agent for the specified project.</para>
      </wadl:doc>

      <request>
         <representation mediaType="application/json">
            <doc>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgent_request-json-http.txt"/>
               <xsdxt:code href="../wadl/samples-v2/cbu-postAgent_request.json"/>
            </doc>
            <param name="name" style="plain" path="$.name" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Name of the agent.
               </wadl:doc>
            </param>
            <param name="version" style="plain" path="$.version" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Version of the agent.
               </wadl:doc>
            </param>
            <param name="host" style="plain" path="$.host" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the host.
               </wadl:doc>
            </param>
                     <param name="flavor" style="plain" path="$.host.flavor" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN"><para>Flavor for the agent. The available flavors are <code>nova</code>, <code>rackspace_dedicated</code>, and <code>rackspace_private_cloud</code>. Depending on the flavor specified, the following notes apply:

                           <itemizedlist>
                              <listitem><para><code>nova</code></para>

                              <itemizedlist>
                                 <listitem><para>Specification of <code>host/region</code> is preferred, but it can be <code>null</code>. </para></listitem>
                                 <listitem><para>Specification of the <code>vault/use_internal</code> is optional. The default value is <code>true</code>.</para></listitem>
                              </itemizedlist></listitem>

                           <listitem><para><code>rackspace_dedicated</code></para>

                              <itemizedlist>
                                 <listitem><para>Specification of <code>host/region</code> is required. </para></listitem>
                                 <listitem><para>Specification of the <code>vault/use_internal</code> is optional. The default value is <code>false</code>.</para></listitem>
                              </itemizedlist></listitem>

                              <listitem><para><code>rackspace_private_cloud</code></para>

                              <itemizedlist>
                                 <listitem><para>Specification of <code>host/region</code> is required. </para></listitem>
                                 <listitem><para>Specification of the <code>vault/use_internal</code> is optional. The default value is <code>true</code>.</para></listitem>
                              </itemizedlist></listitem>

                           </itemizedlist>
                           </para></wadl:doc>
                     </param>
                     <param name="region" style="plain" path="$.host.region" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Region for the agent.
                        </wadl:doc>
                     </param>
                     <param name="machine" style="plain" path="$.host.machine" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Information about the machine.
                        </wadl:doc>
                     </param>
                            <param name="id" style="plain" path="$.host.machine.id" required="true">
                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Machine ID.
                               </wadl:doc>
                            </param>

                     <param name="os" style="plain" path="$.host.os" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Information about the operating system.
                        </wadl:doc>
                     </param>
                            <param name="name" style="plain" path="$.host.os.name" required="true">
                               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Name of the operating system.
                               </wadl:doc>
                            </param>
                           <param name="version" style="plain" path="$.host.os.version" required="true">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                 xml:lang="EN">Version of the operating system.
                              </wadl:doc>
                           </param>
                           <param name="architecture" style="plain" path="$.host.os.architecture" required="true">
                              <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                  xml:lang="EN">Architecture of the operating system.
                              </wadl:doc>
                           </param>
                      <param name="fingerprint" style="plain" path="$.host.fingerprint" required="true">
                         <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN">String determined by the agent.
                         </wadl:doc>
                      </param>
                     <param name="addresses" style="plain" path="$.host.addresses" required="true">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                           xml:lang="EN">Information about the IP addresses for the agent.
                        </wadl:doc>
                     </param>
                              <param name="version" style="plain" path="$.host.addresses.version" required="true">
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">Version of the address (4 for IPv4 or 6 for IPv6).
                                 </wadl:doc>
                              </param>
                              <param name="addr" style="plain" path="$.host.addresses.addr" required="true">
                                 <wadl:doc xmlns="http://docbook.org/ns/docbook"
                                    xml:lang="EN">IP address.
                                 </wadl:doc>
                              </param>
            <param name="rsa_public_key" style="plain" path="$.rsa_public_key" required="true">
               <wadl:doc xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN">Information about the RSA public key.
               </wadl:doc>
            </param>
                  <param name="modulus" style="plain" path="$.rsa_public_key.modulus" required="true">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook"
